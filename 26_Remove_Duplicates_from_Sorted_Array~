class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int start = 0;
        int current;
        while (start < nums.size()){
            current = nums[start++];
            // delete replicates
            while (start < nums.size() && nums[start] == current){
                nums.erase(nums.begin() + start);
            }
        }
        return nums.size();
    }
};